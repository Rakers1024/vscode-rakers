{
  "name": "vscode-rakers",
  "displayName": "vscode-rakers",
  "description": "rakers私人定制插件",
  "version": "0.0.9",
  "scripts": {
    "plop": "plop",
    "vscode:prepublish": "npm run package",
    "vsce:package": "vsce package",
    "pub": "vsce publish",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "release": "npx bumpp --tag --commit"
  },
  "engines": {
    "vscode": "^1.84.0"
  },
  "publisher": "Rakers",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Rakers1024/vscode-rakers"
  },
  "license": "MIT",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "icon": "static/logo.png",
  "contributes": {
    "commands": [
      {
        "title": "在此处打开新工作台",
        "command": "vscode-rakers.openNewInstance"
      },
      {
        "title": "在此处重新打开工作台",
        "command": "vscode-rakers.scopeToHere"
      },
      {
        "title": "复制名称",
        "command": "vscode-rakers.copyFileName"
      },
      {
        "title": "下载文件到此",
        "command": "vscode-rakers.downloadFile"
      },
      {
        "title": "代理克隆GIt仓库",
        "command": "vscode-rakers.proxyClone"
      },
      {
        "title": "创建副本",
        "command": "vscode-rakers.makeFile"
      },
      {
        "title": "使用Git仓库代理",
        "command": "vscode-rakers.gitProxy"
      },
      {
        "title": "取消Git仓库代理",
        "command": "vscode-rakers.gitProxyCancel"
      },
      {
        "title": "切换Vue标签",
        "command": "vscode-rakers.switchVueTag"
      },
      {
        "title": "压缩",
        "command": "vscode-rakers.zip"
      },
      {
        "title": "新建常用文件",
        "command": "vscode-rakers.newFile"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "vscode-rakers.newFile",
          "group": "navigation@6",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "vscode-rakers.openNewInstance",
          "group": "navigation@99"
        },
        {
          "command": "vscode-rakers.scopeToHere",
          "group": "navigation@98",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "vscode-rakers.copyFileName"
        },
        {
          "command": "vscode-rakers.downloadFile",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "vscode-rakers.makeFile"
        },
        {
          "command": "vscode-rakers.zip"
        }
      ],
      "editor/title/context": [
        {
          "command": "vscode-rakers.copyFileName"
        },
        {
          "command": "vscode-rakers.makeFile"
        }
      ],
      "scm/title": [
        {
          "command": "vscode-rakers.gitProxyCancel",
          "group": "inline",
          "title": "Edit",
          "when": "scmProvider == git"
        },
        {
          "command": "vscode-rakers.gitProxy",
          "group": "inline",
          "title": "Edit",
          "when": "scmProvider == git"
        }
      ],
      "commandPalette": [
        {
          "command": "vscode-rakers.openNewInstance",
          "when": "never"
        },
        {
          "command": "vscode-rakers.scopeToHere",
          "when": "never"
        }
      ]
    },
    "keybindings": [
      {
        "command": "vscode-rakers.switchVueTag",
        "key": "shift+cmd+v",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Rakers私人定制插件配置项",
      "properties": {
        "vscode-rakers.proxyClone.proxy": {
          "type": "string",
          "default": "http://127.0.0.1:7890",
          "description": "代理克隆代理地址"
        },
        "vscode-rakers.gitProxy.proxy": {
          "type": "string",
          "default": "http://127.0.0.1:7890",
          "description": "Git代理地址"
        },
        "vscode-rakers.proxyClone.autoOpen": {
          "type": "boolean",
          "default": true,
          "description": "自动打开克隆的路径"
        },
        "vscode-rakers.makeFile.openFileAfterCopy": {
          "type": "boolean",
          "default": true,
          "description": "自动打开新复制的文件"
        },
        "vscode-rakers.makeFile.keepOriginalExtension": {
          "type": "boolean",
          "default": true,
          "description": "保留原始扩展名(未指定下)"
        }
      }
    }
  },
  "devDependencies": {
    "@types/archiver": "^6.0.1",
    "@types/fs-extra": "5.0.1",
    "@types/lodash.escaperegexp": "^4.1.3",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/vscode": "^1.84.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vscode/test-electron": "^2.3.6",
    "axios": "^1.6.0",
    "clipboardy": "^2.3.0",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "plop": "^4.0.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "archiver": "^6.0.1",
    "fs-extra": "^5.0.0",
    "lodash.escaperegexp": "^4.1.2"
  }
}