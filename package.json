{
  "name": "vscode-rakers",
  "displayName": "vscode-rakers",
  "description": "rakers私人定制插件",
  "version": "0.0.5",
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "vsce:package": "vsce package",
    "pub": "vsce publish",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "release": "npx bumpp --tag --commit"
  },
  "engines": {
    "vscode": "^1.84.0"
  },
  "publisher": "Rakers",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Rakers1024/vscode-rakers"
  },
  "license": "MIT",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "icon": "static/logo.png",
  "contributes": {
    "commands": [
      {
        "command": "vscode-rakers.openNewInstance",
        "title": "在此处打开新工作台"
      },
      {
        "command": "vscode-rakers.scopeToHere",
        "title": "在此处重新打开工作台"
      },
      {
        "command": "vscode-rakers.copyFileName",
        "title": "复制名称"
      },
      {
        "command": "vscode-rakers.downloadFile",
        "title": "下载文件到此"
      },
      {
        "command": "vscode-rakers.proxyClone",
        "title": "代理克隆GIt仓库"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "vscode-rakers.openNewInstance",
          "group": "navigation@99"
        },
        {
          "command": "vscode-rakers.scopeToHere",
          "group": "navigation@98",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "vscode-rakers.copyFileName"
        },
        {
          "command": "vscode-rakers.downloadFile",
          "when": "explorerResourceIsFolder"
        }
      ],
      "editor/title/context": [
        {
          "command": "vscode-rakers.copyFileName"
        }
      ],
      "commandPalette": [
        {
          "command": "vscode-rakers.openNewInstance",
          "when": "never"
        },
        {
          "command": "vscode-rakers.scopeToHere",
          "when": "never"
        }
      ]
    }
  },
  "devDependencies": {
    "axios": "^1.6.0",
    "clipboardy": "^2.3.0",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/vscode": "^1.84.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
